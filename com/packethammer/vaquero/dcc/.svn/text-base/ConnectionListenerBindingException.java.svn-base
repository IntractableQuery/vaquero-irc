/**
 * This typically occurs when the DCC manager runs out of ports to use, although
 * it can also occur for other reasons.
 */

package com.packethammer.vaquero.dcc;

public class ConnectionListenerBindingException extends Exception {
    public ConnectionListenerBindingException(String reason) {
        super(reason);
    }    
    
    public ConnectionListenerBindingException(Exception cause) {
        super(cause);
    }   
}
